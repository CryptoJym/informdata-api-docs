# InformData API Documentation Environment Variables
# Copy this file to .env.local for local development

# API Configuration (optional - for live examples)
VITE_API_BASE_URL=https://api.informdata.com
VITE_API_VERSION=v1

# Feature Flags (optional)
VITE_ENABLE_PLAYGROUND=true
VITE_ENABLE_LIVE_EXAMPLES=false

# Analytics (optional)
VITE_VERCEL_ANALYTICS_ID=

# Custom Domain (for production)
VITE_DOCS_URL=https://docs.informdata.com